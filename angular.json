{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "coffee": {
      "projectType": "library",
      "root": "projects/coffee",
      "sourceRoot": "projects/coffee/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/coffee/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/coffee/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/coffee/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/coffee/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngx-coffee-ssr": {
      "projectType": "library",
      "root": "projects/ngx-coffee-ssr",
      "sourceRoot": "projects/ngx-coffee-ssr/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-coffee-ssr/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-coffee-ssr/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-coffee-ssr/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-coffee-ssr/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5b8ffc76-6b7c-481e-84fa-f9c4d38a759e"
  }
}
